{
  "compilerOptions": {
    "lib": [
      "es5",
      "es6"
    ],
    "moduleResolution": "node",
    "outDir": "./build",
    "module": "commonjs",
    "target": "es5",
    "sourceMap": true,
    "esModuleInterop": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true
  },
  "exclude": [
    "node_modules"
  ]
}
